{
  "name": "modelit-k12-website",
  "version": "1.0.0",
  "description": "ModelIt K12 Educational Platform",
  "main": "index.js",
  "homepage": "https://Charlesmartinedd.github.io/modelitk12",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode production",
    "build:dev": "webpack --config webpack.config.js --mode development",
    "start": "node server.js",
    "predeploy": "npm run build && npm run copy-files",
    "deploy": "gh-pages -d public",
    "copy-files": "cp dist/bundle.js public/ && cp -r dist/images public/"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@playwright/test": "^1.56.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.3.0",
    "playwright": "^1.56.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
